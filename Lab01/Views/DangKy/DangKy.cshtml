@model DangKyModel
<h1>Đăng ký thông tin</h1>
<br />
<div class="container p-5 my-5 border">
    <form method="post" asp-action="XuLyDangKy" enctype="multipart/form-data">

        <div class="form-group">
            <label>Họ tên</label>
            <input asp-for="hoTen" class="form-control" />
        </div>
        <br />
        <div class="form-group">
            <label>Ngày sinh</label>
            <input asp-for="ngaySinh" type="date" class="form-control" />
        </div>
        <div class="form-group">
            <label>Hình</label>
            <input name="Fhinh" type="file"  class="form-control" />
        </div>
        <br />
        <button type="submit" class="btn badge-success">Đăng ký</button>
    </form>
  
</div>


